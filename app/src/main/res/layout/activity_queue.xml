<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.almurray.android.almurrayportal.firstPages.queue"
    android:background="@drawable/colback">


    <RelativeLayout
        android:layout_width="600dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="25pt"
        android:layout_marginRight="25pt"
        android:layout_marginLeft="25pt"
        android:layout_marginStart="25pt"
        android:background="@drawable/roundedbutton_white"
        android:layout_centerInParent="true">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/queue"
            android:layout_margin="10pt"
            android:fontFamily="@font/monteserratsemibold"
            android:textAlignment="center"/>
    </RelativeLayout>


</RelativeLayout>
